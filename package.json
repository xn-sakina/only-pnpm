{
  "name": "only-pnpm",
  "version": "1.0.0",
  "description": "Only allow use `pnpm` as package manager",
  "keywords": [
    "pnpm",
    "only-pnpm",
    "only-allow-pnpm",
    "only-allow"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/xn-sakina/only-pnpm#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/xn-sakina/only-pnpm"
  },
  "author": "fz6m",
  "files": [
    "dist"
  ],
  "bin": {
    "only-pnpm": "bin/only-pnpm.js"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "node bin/only-pnpm.js",
    "prepublishOnly": "pnpm build",
    "push": "npm publish --registry=https://registry.npmjs.com/"
  },
  "devDependencies": {
    "@fz6m/eslint-plugin-sakina": "^2.0.2",
    "@types/node": "^17.0.31",
    "@xn-sakina/mental": "^1.0.0",
    "commit-verify": "^1.0.3",
    "eslint": "^8.15.0",
    "husky": "^7.0.4",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "mrm-core": "^7.0.0",
    "v8-compile-cache": "^2.3.0"
  },
  "engines": {
    "pnpm": "^7.0.1",
    "node": ">= 16.14.1",
    "npm": "forbidden, use pnpm",
    "yarn": "forbidden, use pnpm"
  },
  "license": "MIT"
}
